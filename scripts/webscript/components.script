def webixHeader(params)
	return {
		"view": "toolbar",
		"cols": [
			{ "view": "label", "label": params.title, "align": "center" },
			{ "view": "button", "type": "icon", "icon": "times", "width": 30, "click": "$$('" + params.windowId + "').close(); roc.deleteWindow('" + params.windowId + "')" }
		]
	};
end